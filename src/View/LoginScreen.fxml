<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="578.0" prefWidth="367.0" style="-fx-border-radius: 10; -fx-background-color: #da7559; -fx-opacity: .8;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
    <children>
      <AnchorPane layoutX="31.0" layoutY="33.0" prefHeight="513.0" prefWidth="301.0" style="-fx-background-color: #8fb48c; -fx-background-radius: 10;">
         <children>
            <Label fx:id="userNameLabel" layoutX="14.0" layoutY="130.0" text="User Name" textFill="#45281ccc">
               <font>
                  <Font name="Ebrima Bold" size="18.0" />
               </font>
            </Label>
            <Label fx:id="passwordLabel" layoutX="14.0" layoutY="191.0" text="Password" textFill="#45281ccc">
               <font>
                  <Font name="Ebrima Bold" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="userNameField" layoutX="14.0" layoutY="153.0" promptText="* Case-sensitive" />
            <TextField fx:id="passwordField" layoutX="14.0" layoutY="214.0" promptText="* Case-sensitive" />
            <Button fx:id="loginButton" layoutX="115.0" layoutY="270.0" mnemonicParsing="false" onAction="#loginButtonHandler" style="-fx-background-color: #e1bf64;" text="Login" textFill="#45281b">
               <font>
                  <Font name="Ebrima Bold" size="18.0" />
               </font>
            </Button>
            <Label fx:id="loginLabel" layoutX="14.0" layoutY="14.0" prefHeight="102.0" prefWidth="224.0" text="Log into VirtuSched" textFill="#45281ccc" wrapText="true">
               <font>
                  <Font name="Ebrima Bold" size="24.0" />
               </font>
            </Label>
         </children></AnchorPane>
      <Button fx:id="closeButton" layoutX="345.0" layoutY="6.0" mnemonicParsing="false" onAction="#closeButtonAction" prefWidth="7.0" style="-fx-background-color: #e1bf64; -fx-background-radius: 10;" text="X" textAlignment="CENTER" textFill="#45281b">
         <font>
            <Font name="Courier New Bold" size="8.0" />
         </font>
      </Button>
      <Button fx:id="minimizeButton" layoutX="324.0" layoutY="6.0" mnemonicParsing="false" onAction="#minimizeButtonAction" prefWidth="7.0" style="-fx-background-color: #e1bf64; -fx-background-radius: 10;" text="-" textAlignment="CENTER" textFill="#45281b">
         <font>
            <Font name="Courier New Bold" size="8.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
